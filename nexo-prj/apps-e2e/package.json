{
  "name": "@nexo-prj/crm-core-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "crm-core-e2e",
    "implicitDependencies": [
      "crm-core"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "crm-core:build",
          "crm-core:serve"
        ]
      }
    }
  }
}
